<?xml version="1.0"?>
<launch>
    <arg name="bag_file" default="processed.bag"/>
    <node name="rosbag" pkg="rosbag" type="play" args="--clock -l $(find insta360_ros_driver)/bag_processed/$(arg bag_file)" output="screen"/>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find insta360_ros_driver)/config/preview.rviz" output="screen"/>
</launch>